---
---
[{% for dataset in site.datasets %}
  {
    "title": {{ dataset.title | jsonify }}@sk,
    "publisher": "https://data.gov.sk/id/legal-subject/30416094" {% if dataset.notes != "" %},
    "description": {{ dataset.notes | jsonify }}{% endif %}{% if dataset.category != "" %},
    "theme": {{ dataset.category | jsonify }}{% endif %}, 
    "periodicity": {{ dataset.periodicity | jsonify }}, 
    "spatial"
    "keyword"
  {% for resource in dataset.resources %}
    "url": {{ resource.url  | jsonify }}
    "title": {{ resource.name  | jsonify }}
    "format": <{{ resource.format }}>
    {% endfor %}
  }{% unless forloop.last %},{% endunless %}{% endfor %}
]
